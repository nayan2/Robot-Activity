{
	"name": "desktime-hack",
	"displayName": "Desktime Hack",
	"description": "Its a hacking tool for desktime active time tracking app",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:desktime-hack.startDHack",
		"onCommand:desktime-hack.stopDHack"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "desktime-hack.startDHack",
				"title": "Start Desktime Hack"
			},
			{
				"command": "desktime-hack.stopDHack",
				"title": "Stop Desktime Hack"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"electron": "^10.1.1",
		"electron-rebuild": "^2.0.1",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.19.0"
	}
}
